{
  "name": "expo-template-tailwind-pack",
  "description": "A starter pack for Expo projects.",
  "keywords": [
    "expo",
    "template",
    "react-native",
    "tailwind",
    "nativewind",
    "zustand",
    "react-query"
  ],
  "license": "MIT",
  "author": "Abner Israel",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/abnerisrael/expo-template-tailwind-pack"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll  --passWithNoTests",
    "test:pre-push": "node ./scripts/test-pre-push.js",
    "lint": "expo lint --fix",
    "type-check": "tsc --noEmit -p tsconfig.type-check.json",
    "commit": "commit",
    "audit": "npm audit --audit-level=high",
    "prettier:autoformat": "prettier --write",
    "markdown:check": "npx markdown-link-check",
    "doctor": "npx expo-doctor --verbose"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5",
    "@hookform/resolvers": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.74.3",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.8.4",
    "expo": "53.0.9",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.5",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tanstack/eslint-plugin-query": "^5.73.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "lefthook": "^1.11.8",
    "markdown-link-check": "^3.13.7",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "^5.3.3"
  },
  "private": false
}
